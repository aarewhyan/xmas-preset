<?xml version="1.0"?>
<presets xmlns="http://josm.openstreetmap.de/tagging-preset-1.0" shortdescription="Xmas Preset" description="Xmas preset for Vespucci and JOSM, based on the original JOSM version from Jan Tappenbeck">
	<!-- Icons have been derived from mainly from http://osm-icons.org/ Format 
		description: https://josm.openstreetmap.de/wiki/TaggingPresets -->
	<!-- Groups -->
	<group name="Christmas" de.name="Weihnachten" icon="xmas-logo.png">
		<item name="Christmas tree" de.name="Weihnachtsbaum" icon="tree.png">
			<link href="http://wiki.openstreetmap.org/wiki/Key:xmas:feature" de.href="http://wiki.openstreetmap.org/wiki/DE:Key:xmas:feature" fr.href="http://wiki.openstreetmap.org/wiki/FR:Key:xmas:feature"/>
			<!-- Feste Werte -->
			<key key="xmas:feature" value="tree"/>
			<!-- Variable Werte -->
			<text key="xmas:location" text="Location" de.text="Lage" default="" delete_if_empty="true"/>
		</item>
		<item name="Christmas market" de.name="Weihnachtsmarkt" icon="market.png">
			<link href="http://wiki.openstreetmap.org/wiki/DE:Tag:xmas"/>
			<!-- Feste Werte -->
			<key key="xmas:feature" value="market"/>
			<!-- Variable Werte -->
			<text key="xmas:name" text="Name" default="" delete_if_empty="true"/>
			<text key="xmas:location" text="Location" de.text="Lage" default="" delete_if_empty="true"/>
			<text key="xmas:day_date" text="Date range" de.text="Datumsbereich" default="" delete_if_empty="true"/>
			<text key="xmas:opening_hours" text="Opening hours (xmas)" de.text="&#xD6;ffnungszeiten (xmas)" default="" delete_if_empty="true"/>
			<text key="opening_hours" text="Opening hours" de.text="&#xD6;ffnungszeiten" default="" delete_if_empty="true"/>
			<text key="xmas:url" text="Website" default="" delete_if_empty="true"/>
			<text key="xmas:note" text="Note" default="" delete_if_empty="true"/>
		</item>
		<item name="Christmas event" de.name="Weihnachtsanlass" icon="event.png">
			<link href="http://wiki.openstreetmap.org/wiki/Key:xmas:feature" de.href="http://wiki.openstreetmap.org/wiki/DE:Key:xmas:feature" fr.href="http://wiki.openstreetmap.org/wiki/FR:Key:xmas:feature"/>
			<!-- Feste Werte -->
			<key key="xmas:feature" value="event"/>
			<!-- Variable Werte -->
			<text key="xmas:name" text="Name" default="" delete_if_empty="true"/>
			<text key="xmas:location" text="Location" de.text="Lage" default="" delete_if_empty="true"/>
			<text key="xmas:day_date" text="Date range" de.text="Datumsbereich" default="" delete_if_empty="true"/>
			<text key="xmas:opening_hours" text="Opening hours (xmas)" de.text="&#xD6;ffnungszeiten (xmas)" default="" delete_if_empty="true"/>
			<text key="opening_hours" text="Opening hours" de.text="&#xD6;ffnungszeiten" default="" delete_if_empty="true"/>
			<text key="xmas:url" text="Website" default="" delete_if_empty="true"/>
			<text key="xmas:note" text="Note" default="" delete_if_empty="true"/>
		</item>
		<item name="Christmas pyramid" de.name="Weihnachtspyramide" icon="pyramid.png">
			<link href="http://wiki.openstreetmap.org/wiki/Key:xmas:feature" de.href="http://wiki.openstreetmap.org/wiki/DE:Key:xmas:feature" fr.href="http://wiki.openstreetmap.org/wiki/FR:Key:xmas:feature"/>
			<!-- Feste Werte -->
			<key key="xmas:feature" value="pyramid"/>
			<!-- Variable Werte -->
			<text key="xmas:name" text="Name" default="" delete_if_empty="true"/>
			<text key="xmas:location" text="Location" de.text="Lage" default="" delete_if_empty="true"/>
			<text key="xmas:day_date" text="Date range" de.text="Datumsbereich" default="" delete_if_empty="true"/>
			<text key="xmas:opening_hours" text="Opening hours (xmas)" de.text="&#xD6;ffnungszeiten (xmas)" default="" delete_if_empty="true"/>
			<text key="opening_hours" text="Opening hours" de.text="&#xD6;ffnungszeiten" default="" delete_if_empty="true"/>
			<text key="xmas:note" text="Note" de.text="Hinweis" default="" delete_if_empty="true"/>
		</item>
	</group>
</presets>
